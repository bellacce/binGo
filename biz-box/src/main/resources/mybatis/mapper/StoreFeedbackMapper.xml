<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wenhui.project.dal.mybatis.dao.StoreFeedbackMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wenhui.project.dal.mybatis.dataobject.StoreFeedback">
        <id column="id" property="id"/>
        <result column="problem_describe" property="problemDescribe"/>
        <result column="phone" property="phone"/>
        <result column="email" property="email"/>
        <result column="images" property="images"/>
        <result column="videos" property="videos"/>
        <result column="uid" property="uid"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        problem_describe, id, phone, email, images, videos, uid
    </sql>

</mapper>
