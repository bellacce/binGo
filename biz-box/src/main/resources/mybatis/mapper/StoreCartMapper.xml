<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wenhui.project.dal.mybatis.dao.StoreCartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wenhui.project.dal.mybatis.dataobject.StoreCart">
        <id column="id" property="id"/>
        <result column="uid" property="uid"/>
        <result column="goods_name" property="goodsName"/>
        <result column="goods_id" property="goodsId"/>
        <result column="spec_sku_id" property="specSkuId"/>
        <result column="goods_no" property="goodsNo"/>
        <result column="goods_thumb" property="goodsThumb"/>
        <result column="goods_price" property="goodsPrice"/>
        <result column="line_price" property="linePrice"/>
        <result column="number" property="number"/>
        <result column="created_at" property="createdAt"/>
        <result column="updated_at" property="updatedAt"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, uid, goods_name, goods_id, spec_sku_id, goods_no, goods_thumb, goods_price, line_price, number, created_at,
        updated_at
    </sql>

</mapper>
